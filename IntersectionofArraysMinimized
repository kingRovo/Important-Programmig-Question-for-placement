//ToFindIntersectionofArrays
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int i,j;
    int arr1[]={1,3,4,5,7};
    int arr2[]={2,3,5,6};
    int n = sizeof(arr1)/sizeof(arr1[0]);
    int m = sizeof(arr2)/sizeof(arr2[0]);
    //Intersection
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        if(arr1[i]==arr2[j])
        cout<<arr1[i]<<endl;
    }
    
    return 0;
}
